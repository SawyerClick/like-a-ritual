{{! See variable definitions in data/meta.json }}
<title>{{meta.title}}</title>
<meta charset='utf-8'>
<meta name='viewport' content='width=device-width, initial-scale=1'>
<meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'>
<meta name='description' content='Challenging the livelihood of the genre in the streaming age using Spotify playlists.' />
<meta name='news_keywords' content='{{meta.keywords}}' />

<link rel="shortcut icon" href="https://emojipedia-us.s3.dualstack.us-west-1.amazonaws.com/thumbs/120/apple/225/sign-of-the-horns_1f918.png" type="image/x-icon"/>

<meta property='og:title' content='{{meta.title}}' />
<meta property='og:site_name' content='{{meta.siteName}}' />
<meta property='og:url' content='{{meta.url}}' />
<meta property='og:description' content='{{meta.description}}' />
<meta property='og:type' content='article' />
<meta property='og:locale' content='en_US' />

<meta property="og:image" content="https://raw.githubusercontent.com/SawyerClick/SawyerClick.github.io/master/headers/ispunkdead.png">


<meta name='twitter:card' content='summary_large_image' />
